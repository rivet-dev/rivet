{
	"name": "@rivetkit/framework-base",
	"version": "2.0.42-rc.1",
	"description": "Base framework utilities for RivetKit client integrations",
	"license": "Apache-2.0",
	"keywords": [
		"rivetkit",
		"framework",
		"base",
		"utilities",
		"integration"
	],
	"sideEffects": [
		"./dist/chunk-*.js",
		"./dist/chunk-*.cjs"
	],
	"files": [
		"dist",
		"package.json"
	],
	"exports": {
		".": {
			"import": {
				"types": "./dist/mod.d.mts",
				"default": "./dist/mod.mjs"
			},
			"require": {
				"types": "./dist/mod.d.ts",
				"default": "./dist/mod.js"
			}
		}
	},
	"scripts": {
		"build": "tsup src/mod.ts",
		"check-types": "tsc --noEmit"
	},
	"devDependencies": {
		"tsup": "^8.4.0",
		"typescript": "^5.5.2"
	},
	"stableVersion": "0.8.0",
	"dependencies": {
		"@tanstack/store": "^0.7.1",
		"fast-deep-equal": "^3.1.3",
		"rivetkit": "workspace:*"
	}
}
