{
	"name": "@rivetkit/effect",
	"version": "2.0.39",
	"description": "Effect integration for RivetKit actors",
	"license": "Apache-2.0",
	"keywords": [
		"rivetkit",
		"effect",
		"effect-ts",
		"actors",
		"functional"
	],
	"sideEffects": [
		"./dist/chunk-*.js",
		"./dist/chunk-*.cjs"
	],
	"files": [
		"dist",
		"package.json"
	],
	"type": "module",
	"exports": {
		".": {
			"import": {
				"types": "./dist/mod.d.ts",
				"default": "./dist/mod.js"
			},
			"require": {
				"types": "./dist/mod.d.cts",
				"default": "./dist/mod.cjs"
			}
		}
	},
	"scripts": {
		"build": "tsup src/mod.ts",
		"check-types": "tsc --noEmit",
		"test": "vitest run"
	},
	"dependencies": {
		"rivetkit": "workspace:^"
	},
	"peerDependencies": {
		"effect": "^3.0.0"
	},
	"devDependencies": {
		"@hono/node-server": "^1.14.0",
		"@hono/node-ws": "^1.1.0",
		"@types/node": "^22.13.9",
		"effect": "^3.19.9",
		"tsup": "^8.4.0",
		"typescript": "^5.7.3",
		"vite-tsconfig-paths": "^5.1.4",
		"vitest": "^3.2.4"
	},
	"stableVersion": "0.8.0"
}
