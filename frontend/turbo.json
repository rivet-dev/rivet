{
	"$schema": "https://turbo.build/schema.json",
	"extends": ["//"],
	"tasks": {
		"build": {
			"dependsOn": ["build:inspector"],
			"outputs": ["dist/**"]
		},
		"build:engine": {
			"dependsOn": ["^build"],
			"env": ["VITE_APP_*", "DEPLOYMENT_TYPE"],
			"outputs": ["dist/**"]
		},
		"build:inspector": {
			"dependsOn": ["^build"],
			"env": ["VITE_APP_*", "DEPLOYMENT_TYPE"],
			"outputs": ["dist/**"]
		},
		"build:cloud": {
			"dependsOn": ["^build"],
			"env": ["VITE_APP_*", "DEPLOYMENT_TYPE"],
			"outputs": ["dist/**"]
		},
		"dev": {
			"dependsOn": ["^build"],
			"env": ["VITE_APP_*"],
			"persistent": true
		}
	}
}
