{
	"name": "@rivet-gg/icons",
	"version": "2.0.37",
	"description": "Icon library for Rivet products. Licensed exclusively for use in Rivet products and services only.",
	"license": "SEE LICENSE IN LICENSE",
	"private": false,
	"sideEffects": false,
	"files": [
		"LICENSE",
		"scripts",
		"manifest.json",
		"src",
		"dist"
	],
	"scripts": {
		"generate": "npm run gen:manifest && npm run gen:vendor",
		"gen:vendor": "node scripts/vendor-icons.js",
		"gen:manifest": "node scripts/generate-manifest.js"
	},
	"engines": {
		"node": ">=18"
	},
	"peerDependencies": {
		"@fortawesome/fontawesome-svg-core": "^6.5.2",
		"@fortawesome/free-brands-svg-icons": "^6.5.2",
		"@fortawesome/free-solid-svg-icons": "^6.5.2",
		"@fortawesome/react-fontawesome": "^0.2.2",
		"react": "^19",
		"react-dom": "^19"
	},
	"type": "module",
	"main": "./dist/index.js",
	"types": "./src/index.gen.ts",
	"exports": {
		".": {
			"types": "./src/index.gen.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.js",
			"default": "./dist/index.js"
		}
	},
	"devDependencies": {
		"dedent": "^1.7.0",
		"esbuild": "^0.25.9",
		"local-pkg": "^0.5.1"
	}
