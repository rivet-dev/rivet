---
// HACK: This page allows us to put tools in the sidebar but redirect to a different page.
// We can't use `href` since that will change which sidebar/tab is active when on the tool's page.

export function getStaticPaths() {
	return [{ params: { tool: 'actors' } }];
}

const { tool } = Astro.params;
return Astro.redirect(`/docs/${tool}/`, 301);
---
