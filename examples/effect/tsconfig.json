{
	"compilerOptions": {
		"target": "esnext",
		"lib": ["esnext"],
		"module": "esnext",
		"moduleResolution": "bundler",
		"types": ["node"],
		"noEmit": true,
		"strict": true,
		"skipLibCheck": true,
		"allowImportingTsExtensions": true,
		"rewriteRelativeImportExtensions": true,
		"plugins": [{ "name": "@effect/language-service" }]
	},
	"include": ["src/**/*"]
}
