{
	"name": "example-stream",
	"version": "2.0.21",
	"type": "module",
	"scripts": {
		"dev:backend": "srvx --import tsx src/server.ts",
		"dev:frontend": "vite",
		"dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
		"start": "srvx --static=../frontend/dist dist/server.js",
		"check-types": "tsc --noEmit",
		"test": "vitest",
		"build:frontend": "vite build",
		"build:backend": "tsup",
		"build": "npm run build:backend && npm run build:frontend"
	},
	"devDependencies": {
		"@hono/node-server": "^1.19.7",
		"@hono/node-ws": "^1.3.0",
		"@types/node": "^20.0.0",
		"@types/react": "^18.2.0",
		"@types/react-dom": "^18.2.0",
		"@vitejs/plugin-react": "^4.0.0",
		"concurrently": "^8.2.0",
		"srvx": "^0.10.0",
		"tsup": "^8.5.1",
		"tsx": "^4.0.0",
		"typescript": "^5.0.0",
		"vite": "^5.0.0",
		"vitest": "^3.1.1"
	},
	"dependencies": {
		"@rivetkit/react": "*",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"rivetkit": "*"
	},
	"template": {
		"technologies": [
			"typescript"
		],
		"tags": [
			"real-time"
		],
		"frontendPort": 5173
	},
	"license": "MIT"
}
