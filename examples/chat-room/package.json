{
	"name": "chat-room",
	"version": "2.0.21",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
		"dev:backend": "npx srvx --import tsx src/server.ts",
		"dev:frontend": "vite",
		"dev:cli": "tsx src/scripts/cli.ts",
		"check-types": "tsc --noEmit",
		"test": "vitest run",
		"build": "tsc && vite build"
	},
	"devDependencies": {
		"@rivetkit/react": "workspace:*",
		"@types/node": "^22.13.9",
		"@types/prompts": "^2",
		"@types/react": "^18.2.0",
		"@types/react-dom": "^18.2.0",
		"@vitejs/plugin-react": "^4.2.0",
		"concurrently": "^8.2.2",
		"prompts": "^2.4.2",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"tsx": "^3.12.7",
		"typescript": "^5.5.2",
		"vite": "^5.0.0",
		"vitest": "^3.1.1"
	},
	"dependencies": {
		"hono": "^4.11.3",
		"rivetkit": "workspace:*"
	},
	"stableVersion": "0.8.0",
	"template": {
		"technologies": [
			"react",
			"typescript"
