{
	"name": "example-experimental-durable-streams-ai-agent",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"scripts": {
		"dev:backend": "srvx --import tsx src/server.ts",
		"dev:frontend": "vite",
		"dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
		"dev:streams": "tsx --watch src/streams-server/server.ts",
		"start": "srvx --static=../frontend/dist dist/server.js",
		"check-types": "tsc --noEmit",
		"test": "vitest run",
		"build:frontend": "vite build",
		"build:backend": "tsup",
		"build": "npm run build:backend && npm run build:frontend"
	},
	"devDependencies": {
		"@types/node": "^22.13.9",
		"@types/react": "^18.2.0",
		"@types/react-dom": "^18.2.0",
		"@vitejs/plugin-react": "^4.2.0",
		"concurrently": "^8.2.2",
		"tsup": "^8.5.1",
		"tsx": "^3.12.7",
		"typescript": "^5.5.2",
		"vite": "^5.0.0",
		"vitest": "^3.1.1"
	},
	"dependencies": {
		"@ai-sdk/anthropic": "^1.0.0",
		"@durable-streams/client": "https://pkg.pr.new/rivet-dev/durable-streams/@durable-streams/client@0323b8b",
		"@durable-streams/server": "https://pkg.pr.new/rivet-dev/durable-streams/@durable-streams/server@0323b8b",
		"@durable-streams/writer": "https://pkg.pr.new/rivet-dev/durable-streams/@durable-streams/writer@0323b8b",
		"@hono/node-server": "^1.19.7",
		"@hono/node-ws": "^1.3.0",
		"@rivetkit/react": "*",
		"ai": "^4.0.38",
		"hono": "^4.11.3",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"rivetkit": "*",
		"srvx": "^0.10.0",
		"zod": "^4.1.0"
	},
	"template": {
		"technologies": [
			"typescript",
			"durable-streams"
		],
		"tags": [
			"ai",
			"real-time",
			"experimental"
		],
		"frontendPort": 5173
	},
	"license": "MIT"
